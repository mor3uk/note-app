#include <iostream>
#include <string>
using namespace std;

int dif[26];

char cypherSym(char symbol)
{
  int code = symbol + 1;
  int start = 97;

  if (code < start) {
    start = 65;
  }

  code = code - start;

  for (int i = 0; i < 26; i++) {
    if (code == dif[i]) {
      code = i + start;
      break;
    }
  }

  symbol = (char)code;


  return symbol;
}

int main()
{
  for (int i = 0; i < 26; i++) {
    cin >> dif[i];
  }

  string strNew;
  getline(cin, strNew);
  getline(cin, strNew);

  for (int i = 0; i < strNew.size(); i++) {
    if (isalpha(strNew[i])) {
      strNew[i] = cypherSym(strNew[i]);
    }
  }

  cout << strNew;
} 
