import java.util.ArrayList;
import java.util.Scanner;

class solver {
	  public static void main (String[] args) {
	    Scanner sc = new Scanner(System.in);
	    
	    int n = sc.nextInt();
	    sc.nextLine();
	    
	    String[] words = new String[n];
	    for (int i = 0; i < n; i++) {
	    	words[i] = sc.nextLine();
	    }
	    
	    String string = sc.nextLine();
	    StringBuilder str = new StringBuilder(string);
	    
	    int start = 0;
	    int end = 0;
	    char temp;
	    String strTemp;
	    
	    for (int i = 0; i < str.length(); i++) {
	    	if (i == 0 && Character.isAlphabetic(str.charAt(i)) || Character.isAlphabetic(str.charAt(i))
	    			&& !Character.isAlphabetic(str.charAt(i - 1))) {
	    		start = i;
	    	}
	    	if (i == str.length() - 1 && Character.isAlphabetic(str.charAt(i)) || Character.isAlphabetic(str.charAt(i))
	    			&& !Character.isAlphabetic(str.charAt(i + 1))) {
	    		end = i;
	    		
	    		strTemp = "";
	    		for (int j = start; j <= end; j++) {
	    			strTemp += str.charAt(j);
	    		}
	    		
	    		if (checkWord(strTemp, words)) {
	    			str.delete(start, end + 1);
	    			
	    			for (int j = start; j <= end; j++) {
	    				str.insert(start, '*');
	    			}
	    		}
	    		
	    	}
	    }
	    
	    System.out.println(str);
	     
	  }
	  
	  public static boolean checkWord (String str, String[] words) {
		  for (int i = 0; i < words.length; i++) {
			  if (words[i].toLowerCase().equals(str.toLowerCase())) {
				  return true;
			  }
		  }
		  
		  return false;
	  }
}
 
