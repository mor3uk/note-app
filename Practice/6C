import java.util.Scanner;

class solver {
	  public static void main (String[] args) {
	    Scanner sc = new Scanner(System.in);
	   
	    String str = sc.nextLine();
	    StringBuilder strNew = new StringBuilder(str);
	    
	    for (int i = 0; i < strNew.length(); i++) {
	    	if (Character.isAlphabetic(strNew.charAt(i))) {
	    		strNew.setCharAt(i, cypherSym(strNew.charAt(i)));
	    	}
	    }
	    
	    System.out.println(strNew);
	   
	    
	   
	    
	    
	  }
	  
	  public static char cypherSym (char symbol) {
		  int code = symbol;
		  int start = 97;
		  int end = 122;
		  int dif;
		  
		  if (code < start) {
			  start = 64;
			  end = 91;
		  }
		  
		  dif = code - start;
		  code = end - dif;
		  
		  symbol = (char) code;
		  
		  
		  
		  return symbol;
		  
	  }
	  
	  
	} 
